<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.beesinergi.mapper.PoOutDetailMapper">

	<insert id="insert" parameterType="com.beesinergi.model.PoOutDetail">
		insert into po_out_detail ( 
			fk_po_out,
			fk_product,
			qty,
			req_qty
		) values (
			#{fkPoOut},
			#{fkProduct},
			#{qty},
			#{reqQty}
      )
	</insert>
	
	<delete id="deleteByFkPoOut" parameterType="java.lang.Integer">
		delete from po_out_detail where fk_po_out = #{fkPoOut}
	</delete>

</mapper>